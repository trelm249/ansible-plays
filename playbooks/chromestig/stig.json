//Stig requirements - located at /etc/opt/chrome/policies/managed/stig.json
{
  "HomepageLocation": "http://www.public.navy.mil/fltfor/cnmoc/Pages/home.aspx",
  "SafeBrowsingEnabled": true,
  "RemoteAccessHostFirewallTraversal": false,
  "DefaultNotificationsSetting": 2,
  "DefaultPopupsSetting": 2,
  "PopupsAllowedForUrls": ["[*.]mil", "[*.]gov", "[*.]taleo.net"],
  "PluginsAllowedForUrls": ["[*.]mil", "[*.]gov", "[*.]taleo.net"],
  "DefaultGeolocationSetting": 2,
  "ExtensionInstallBlacklist" : "*",
  "ExtensionInstallWhitelist": ["oiigbmnaadbkfbmpbfijlflahbdbdgdf"],
  "DefaultSearchProviderName": "Google Encrypted",
  "DefaultSearchProviderSearchURL": "https://www.google.com/#q={searchTerms}",
  "DefaultSearchProviderEnabled": true,
  "PasswordManagerEnabled": false,
  "AllowOutdatedPlugins": false,
  "AllowDeletingBrowserHistory": false,
  "BlockThirdPartyCookies": true,
  "BackgroundModeEnabled": false,
  "Disable3DAPIs": true,
  "SyncDisabled": true,
  "URLBlacklist": "javascript://*",
  "AutoFillEnabled": false,
  "CloudPrintProxyEnabled": false,
  "NetworkPredictionOptions": 0,
  "MetricsReportingEnabled": false,
  "SearchSuggestEnabled": false,
  "ImportSavedPasswords": false,
  "IncognitoModeAvailability": 1,
  "DisabledPlugins": "*",
  "EnabledPlugins": ["Java*", "Shockwave Flash", "Chrome PDF Viewer", "Silverlight"],
  "EnableOnlineRevocationChecks": true,
  "SavingBrowserHistoryDisabled": false,
  "DefaultPluginsSetting": 3,
  "PromptForDownloadLocation": true,
  "DownloadRestrictions": 1,
  "AutofillCreditCardEnabled": false,
}
